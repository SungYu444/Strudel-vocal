{
  "project": "Strudel Vocal Synthesizer",
  "version": "1.0.0",
  "settings": {
    "sampleRate": 44100,
    "bitDepth": 16,
    "channels": 2
  },
  "instruments": [
    {
      "name": "Piano",
      "type": "keyboard",
      "settings": {
        "attack": 0.01,
        "decay": 0.1,
        "sustain": 0.8,
        "release": 0.5
      }
    },
    {
      "name": "Violin",
      "type": "string",
      "settings": {
        "bowPressure": 0.7,
        "vibratoDepth": 0.3
      }
    }
  ],
  "effects": [
    {
      "name": "Reverb",
      "settings": {
        "roomSize": 0.5,
        "damping": 0.2
      }
    },
    {
      "name": "Delay",
      "settings": {
        "time": 0.3,
        "feedback": 0.4
      }
    }
  ]
}